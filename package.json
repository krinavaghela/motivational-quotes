{
  "name": "motivational-quotes-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "build:gh": "GITHUB_PAGES=true GITHUB_PAGES_REPO=motivational-quotes next build",
    "export:gh": "GITHUB_PAGES=true GITHUB_PAGES_REPO=motivational-quotes next export",
    "deploy:gh": "npm run build:gh && npm run export:gh",
    "publish:gh": "npm run deploy:gh && GITHUB_PAGES_REPO=motivational-quotes npx gh-pages -d out -b gh-pages"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "next": "^12.3.4",
    "@mui/material": "^5.15.0",
    "@mui/icons-material": "^5.15.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^11.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^12.3.4",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.5.4"
  }
}
